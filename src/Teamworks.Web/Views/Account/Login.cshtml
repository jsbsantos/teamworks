@model Teamworks.Web.Controllers.Web.Login
@{
    ViewBag.Title = "teamworks";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div>
    <form  method="POST" action="@Url.Action("Login", "Account")"  class="form-inline control-group" name="login">
        <input name="username" placeholder="Username"/>
        <input name="password" placeholder="Password" type="password"/>
        <button class="btn btn-small" type="submit" value="login">Login</button>
    </form>
</div>